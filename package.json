{
  "name": "magicbox",
  "version": "0.0.1",
  "description": "description",
  "author": "codacy20",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "serve": "ts-node -r tsconfig-paths/register src/main.ts",
    "serve:dev": "nodemon",
    "serve:debug": "nodemon --config nodemon-debug.json",
    "preserve:prod": "rimraf dist && npm run build",
    "serve:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nest-middlewares/express-session": "^5.1.1",
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/mongoose": "^5.2.2",
    "@nestjs/swagger": "^2.5.1",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "dotenv": "^6.2.0",
    "express-session": "^1.15.6",
    "hmac-validator": "^0.1.0",
    "mongoose": "^5.4.7",
    "reflect-metadata": "^0.1.12",
    "request-promise": "^4.2.2",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "specified": "^0.3.0",
    "typescript": "^3.0.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "jest": "^24.1.0",
    "lodash": "^4.17.11",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engine": {
    "node": ">=8.10.0"
  }
}
